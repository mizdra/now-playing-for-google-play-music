{
  "name": "#NowPlaying for Google Play Music",
  "version": "0.0.1",
  "description": "Share music playing on Google Play Music to SNS.",
  "content_scripts": [
    {
      "matches": ["https://play.google.com/*"],
      "run_at": "document_idle",
      "css": ["contentStyle.css"],
      "js": ["contentScript.js"]
    }
  ],
  "icons": { 
    "128": "icon_128.png" 
  },
  "manifest_version": 2
}
