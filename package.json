{
  "name": "now-playing-for-google-play-music",
  "version": "1.2.0",
  "description": "Share music playing on Google Play Music to SNS.",
  "main": "index.js",
  "repository": "https://github.com/mizdra/now-playing-for-google-play-music",
  "author": "mizdra <pp.mizdra@gmail.com>",
  "license": "MIT",
  "scripts": {
    "build": "run-p -l build:*",
    "build:ext": "webpack --config webpack.ext.js --mode production",
    "build:web": "webpack --config webpack.web.js --mode production",
    "dev": "run-p -l dev:*",
    "dev:ext": "webpack --watch --config webpack.ext.js --mode development",
    "dev:web": "webpack-dev-server --config webpack.web.js --mode development",
    "check": "run-p -l -c 'check:tsc --pretty' check:prettier",
    "check:tsc": "tsc --noEmit",
    "check:prettier": "prettier --check 'src/**/*.{js,ts,tsx,css,html}'"
  },
  "devDependencies": {
    "copy-webpack-plugin": "^5.1.1",
    "css-loader": "^3.4.2",
    "html-webpack-plugin": "^3.2.0",
    "npm-run-all": "^4.1.5",
    "prettier": "^1.19.1",
    "style-loader": "^1.1.3",
    "ts-loader": "^6.2.1",
    "typescript": "^3.8.3",
    "webpack": "^4.41.5",
    "webpack-cli": "^3.3.11",
    "webpack-dev-server": "^3.10.3",
    "webpack-merge": "^4.2.2",
    "workbox-webpack-plugin": "^5.0.0"
  },
  "dependencies": {
    "@types/chrome": "^0.0.93",
    "@types/react": "^16.9.23",
    "@types/react-dom": "^16.9.5",
    "milligram": "^1.3.0",
    "react": "^16.13.0",
    "react-dom": "^16.13.0",
    "react-markdown": "^4.3.1"
  }
}
