  "author": "mizdra <pp.mizdra@gmail.com>",
  "license": "MIT",
  "scripts": {
    "build": "run-p -l build:*",
    "build:ext": "webpack --config webpack.ext.js --mode production",
    "build:web": "webpack --config webpack.web.js --mode production",
    "dev": "run-p -l dev:*",
    "dev:ext": "webpack --watch --config webpack.ext.js --mode development",
    "dev:web": "webpack-dev-server --config webpack.web.js --mode development",
    "check": "run-p -l -c 'check:tsc --pretty' check:prettier",
    "check:tsc": "tsc --noEmit",
    "check:prettier": "prettier --check 'src/**/*.{js,ts,tsx,css,html}'"
  },
  "devDependencies": {
    "copy-webpack-plugin": "^5.0.3",
    "css-loader": "^3.0.0",
    "html-webpack-plugin": "^3.2.0",
    "npm-run-all": "^4.1.5",
    "prettier": "^1.19.1",
    "style-loader": "^0.23.1",
    "ts-loader": "^6.0.3",
    "typescript": "^3.5.2",
    "webpack": "^4.34.0",
    "webpack-cli": "^3.3.4",
    "webpack-dev-server": "^3.7.2",
    "webpack-merge": "^4.2.1",
    "workbox-webpack-plugin": "^4.3.1"
  },
  "dependencies": {
    "@types/chrome": "^0.0.86",
    "@types/react": "^16.8.23",
    "@types/react-dom": "^16.8.4",
    "milligram": "^1.3.0",
    "react": "^16.8.6",
    "react-dom": "^16.12.0",
    "react-markdown": "^4.1.0"
  }
}
